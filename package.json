{
  "name": "vue3store",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "s3": "aws s3 cp dist s3://shop-demo2.christsao.me --recursive --acl=public-read --profile=default",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.6",
    "core-js": "^3.22.0",
    "element-plus": "^2.1.9",
    "pug": "^3.0.2",
    "swiper": "^8.1.0",
    "unplugin-icons": "^0.13.4",
    "vite-plugin-pages": "^0.25.0",
    "vue": "^3.2.29",
    "vue-inner-image-zoom": "^2.0.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.1",
    "@iconify-json/carbon": "^1.1.1",
    "@iconify-json/ic": "^1.1.2",
    "@iconify-json/ion": "^1.1.1",
    "@iconify-json/ph": "^1.1.1",
    "@iconify/json": "^2.1.79",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.22",
    "@unocss/extractor-pug": "^0.26.2",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "sass": "^1.49.9",
    "typescript": "~4.5.5",
    "unocss": "^0.26.2",
    "unplugin-auto-import": "^0.6.0",
    "unplugin-vue-components": "^0.17.18",
    "vite": "^2.7.13",
    "vue-tsc": "^0.31.1"
  }
